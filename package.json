{
  "name": "@pedroloch/components",
  "version": "0.0.33",
  "description": "Components by Pedro Loch",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/components.umd.js",
  "module": "./dist/components.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./src": "./src/index.ts",
    ".": {
      "import": "./dist/components.es.js",
      "require": "./dist/components.umd.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "scripts": {
    "dev": "vite",
    "serve": "vite preview",
    "rollup": "rollup -c",
    "build": "node scripts/build.js && npm run build:css",
    "build:css": "tailwindcss -o dist/styles.css --minify",
    "rebuild": "npm run clean && npm run build",
    "pub": "npm run release && npm run rebuild && npm publish --access public",
    "release": "standard-version",
    "clean": "./scripts/clean.sh",
    "gen": "node ./scripts/generate-color.js"
  },
  "dependencies": {
    "@headlessui/vue": "^1.4.1",
    "@vueuse/core": "^6.4.1",
    "maska": "^1.4.5",
    "nanoid": "^3.1.25",
    "vue": "3.2.9"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.5",
    "@tailwindcss/forms": "^0.3.3",
    "@types/node": "^16.6.1",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vitejs/plugin-vue-jsx": "^1.1.7",
    "@vue/compiler-sfc": "^3.0.5",
    "autoprefixer": "^10.3.1",
    "cssnano": "^5.0.8",
    "execa": "^5.1.1",
    "postcss": "^8.3.6",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "standard-version": "^9.3.1",
    "tailwindcss": "^2.2.7",
    "typescript": "^4.4.2",
    "vite": "^2.4.4",
    "vue-tsc": "^0.2.2"
  },
  "author": "Pedro Loch",
  "license": "MIT"
}
